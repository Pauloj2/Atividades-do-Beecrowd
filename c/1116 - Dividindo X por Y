#include <stdio.h>

int main () {

    int num, i;
    double x, y, div = 0;

    scanf("%d", &num);

    for(i=0; i<num; i++){
        scanf("%lf %lf", &x, &y);
        if(y == 0){
                printf("divisao impossivel\n");
        }else{
            div = x / y;
            printf("%.1lf\n", div);
        }

    }

    return 0;
}
