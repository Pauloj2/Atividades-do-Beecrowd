#include <stdio.h>

int main () {

   double num, soma = 0, media = 0;
   int i, cont = 0;

   for(i=0; i<6; i++){
      scanf("%lf", &num);
      if(num > 0){
         cont++;
         soma = soma + num;
      }
   }
   media = soma / cont;
   printf("%d valores positivos\n", cont);
   printf("%.1lf\n", media);

   return 0;
}
