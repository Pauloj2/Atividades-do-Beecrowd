#include <stdio.h>

int main () {
    int num, alcool = 0, gasolina = 0;
    int diesel = 0;

    while (num != 4){
        scanf("%d", &num);

        if (num == 1){
            alcool++;
        }else if (num == 2){
            gasolina++;
        }else if (num == 3){
            diesel++;
        }
    }
    printf ("MUITO OBRIGADO\n");
    printf ("Alcool: %d\n", alcool);
    printf ("Gasolina: %d\n", gasolina);
    printf ("Diesel: %d\n", diesel);

return 0;
}
